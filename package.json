{
  "name": "incr-regex-package",
  "version": "1.0.1",
  "description": "An incremental regular expression parser in JavaScript; useful for input validation, RegExp",
  "main": "lib/index.js",
  "scripts": {
    "test": "mocha -R spec  --compilers js:babel-register src/test/*.js",
    "build": "babel src --out-dir lib",
    "buildx": "babel src --out-dir lib && cp -R lib/*  ../react-rxinput/node_modules/incr-regex-package/lib/",
    "watch:js": "babel src --out-dir lib --watch ",
    "lint": "jshint src",
    "pack": "node node_modules/webpack/bin/webpack",
    "localdeploy": "cp -R lib/*  ../react-rxinput/node_modules/incr-regex-package/lib/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nurulc/incr-regex-package.git"
  },
  "keywords": [
    "regexp",
    "incremental",
    "JavaScript",
    "validation",
    "form",
    "field",
    "input",
    "mask",
    "pattern",
    "matching"
  ],
  "devDependencies": {
    "@babel/cli": "^7.7.4",
    "@babel/core": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "chai": "^3.4.1",
    "jshint": "^2.9.2",
    "mocha": "*",
    "webpack": "*"
  },
  "dependencies": {},
  "author": "Nurul Choudhury",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nurulc/incr-regex-package/issues"
  },
  "homepage": "https://github.com/nurulc/incr-regex-package#readme",
  "tonicExampleFilename": "example.js"
}
