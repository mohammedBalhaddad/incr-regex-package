{
  "name": "incr-regex-package",
  "version": "0.1.1",
  "description": "An incremental regular expression parser in JavaScript; useful for input validation",
  "main": "lib/index.js",
  "scripts": {
    "test": "mocha -R spec --compilers js:mocha-babel src/test/*.js",
    "build": "babel --presets es2015 src --out-dir lib && cp -R lib/*  ../react-maskedinput/node_modules/incr-regex-package/lib/",
    "watch:js": "babel src --out-dir lib --watch ",
    "lint" : "jshint src",
    "localdeploy": "cp -R lib/*  ../react-maskedinput/node_modules/incr-regex-package/lib/"
    
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nurulc/incr-regex-package.git"
  },
  "keywords": [
    "regex",
    "incremental",
    "JavaScript",
    "validation",
    "form",
    "field",
    "input",
    "mask",
    "pattern",
    "matching"
  ],
  "devDependencies": {
    "babel": "^6.1.18",
    "babel-cli": "^6.2.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.1.18",
    "babel-register": "^6.7.2",
    "babel-runtime": "^6.2.0",
    "jshint": "^2.9.2",
    "mocha": "^2.3.2",
    "mocha-babel": "^3.0.0",
    "webpack": "^1.13.1"
  },
  "dependencies": {
    "chai": "^3.4.1"
  },
  "author": "Nurul Choudhury",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nurulc/incr-regex-package/issues"
  },
  "homepage": "https://github.com/nurulc/incr-regex-package#readme"
}
